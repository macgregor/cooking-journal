{
    "plugins": [ "theme-api", "page-toc" ],
    "pluginsConfig": {
      "theme-api": {
            "theme": "dark"
        },
      "gitbooks-summary-gen":{
            "exclude": ["templates", "README.md", "SUMMARY.md", "2-cookbooks"],
            "cleanup-section-names": true,
            "strip-numbers": true,
            "sort-entry-by-title": true
      },
      "markdown-spellcheck":{
            "exclude": ["**/templates/**"]
      }
    }
}
